    },
    "industry": {
      "type": "string"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "agents"
    },
    "companies": {
      "type": "relation",
      "relation": "manyToMany",
