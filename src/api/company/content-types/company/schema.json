  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "website": {
      "type": "string"
    },
    "podcast_episodes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::podcast-episode.podcast-episode",
      "inversedBy": "companies"
    },
    "agents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::agent.agent",
      "mappedBy": "companies"
    },
    "mcp_servers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::mcp-server.mcp-server",
      "mappedBy": "companies"
    }
  }
