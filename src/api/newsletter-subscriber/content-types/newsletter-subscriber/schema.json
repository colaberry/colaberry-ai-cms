{
  "kind": "collectionType",
  "collectionName": "newsletter_subscribers",
  "info": {
    "singularName": "newsletter-subscriber",
    "pluralName": "newsletter-subscribers",
    "displayName": "Newsletter Subscriber"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "default": "subscribed",
      "enum": [
        "subscribed",
        "unsubscribed",
        "bounced"
      ]
    },
    "sourcePage": {
      "type": "string"
    },
    "sourcePath": {
      "type": "string"
    },
    "subscribedAt": {
      "type": "datetime"
    },
    "unsubscribedAt": {
      "type": "datetime"
    },
    "metadata": {
      "type": "json"
    }
  }
}
